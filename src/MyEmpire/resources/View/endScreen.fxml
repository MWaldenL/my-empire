<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="MyEmpire.Controller.EndScreenController"
            styleClass="background">

    <VBox fx:id="centerBox" alignment="CENTER"
          AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0"
          AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0"/>

    <Label styleClass= "footer"
           text="Lua &#38; So CCPROG3-S15 2019"
           AnchorPane.bottomAnchor="10"
           AnchorPane.leftAnchor="10" />

    <Button id="toggleMute"
            fx:id="btnMusic" onAction="#toggleMusic"
            AnchorPane.bottomAnchor="10"
            AnchorPane.rightAnchor="10" />
</AnchorPane>
